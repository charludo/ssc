%ignore /[ \t\n\f\r]+/

source : proposition+

proposition : expression COMPARISON (expression | proposition)

expression : value | value OPERATOR expression

?value : FIELD
	   | NUMBER

FIELD : /[a-i][1-9]/
NUMBER : /\d+/

OPERATOR : "+"
		 | "-"
		 | "|"

COMPARISON : "="
		   | ">"
		   | "<"
		   | ">="
		   | "<="
		   | "!="
